.home-container {
  width: 100%;
  height: 100vh;
  background: rgb(246, 246, 246);
  box-sizing: border-box;
}
.scroll-container {
  height: 552rpx;
  padding: 0 30rpx;
  box-sizing: border-box;
  padding-top: 24rpx;
}
.todo-list {
  width: 690rpx;
  height: 72rpx;
  background: #fff;
  border-radius: 5rpx;
  display: flex;
  align-items: center;
  box-sizing: border-box;
  margin-bottom: 24rpx;
}
.circle {
  width: 30rpx;
  height: 30rpx;
  border: 1px solid #F6705A;
  border-radius: 50%;
  margin: 0 25rpx;
}
.create-tomato {
  width: 710rpx;
  background: #EEF7F6;
  box-shadow: 0px -2px 8px 0px rgba(0,0,0,0.10);
  margin: 0 auto;
  margin-top: 66rpx;
  position: relative;
  border-top: 1rpx solid transparent;
  border-radius: 10rpx;
}
.create-button {
  position: absolute;
  left: 50%;
  transform: translate(-50%, -50%);
}
.no-login {
  font-size: 20rpx;
  color: #F6705A;
  margin-top: 87rpx;
  text-align: center;
}
.action-tomato {
  width: 269rpx;
  height: 269rpx;
  display: flex;
  margin: 0 auto;
  margin-top: 27rpx;
}